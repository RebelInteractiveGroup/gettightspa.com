{% extends 'layouts/base.njk' %}

{% block head %}
<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW"> 
{% endblock %}

{% block content %}
  <main class="content content-service service-{{ permalink | slug }}" id="main-content" tabindex="-1">
    <article>
      {% for item in components %}
        {% include "partials/components/sections/" + item.type + ".njk" %}
      {% endfor %}
    </article>
  </main>
{% endblock %}


