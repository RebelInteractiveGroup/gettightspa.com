{
  "colors": {
    "themeColors": {
      "primary": "#F89521",
      "secondary": "#FFCF0F",
      "success": "#71A546",
      "info": "#17a2b8",
      "warning": "#ffc107",
      "danger": "#dc3545",
      "light": "#f8f9fa",
      "dark": "#343a40"
    },
    "bodyColors": {
      "bodyBg": "#ffffff",
      "body-color": "#333333",
      "sidebar-bg": "#f8f9fa",
      "sidebar-color": "#404040",
      "headings-color": "#404040",
      "footer-bg": "#304F17",
      "footer-color": "#404040"
    },
    "headerColors": {
      "header-bg": "#ffffff",
      "navbar-bg": "#ffffff"
    }
  },
  "global-settings": {
    "enable-rounded": "true",
    "enable-shadows": "true",
    "enable-gradients": false,
    "enable-responsive-font-sizes": "true"
  },
  "fonts": {
    "baseFontName": "Spartan",
    "headingsFontName": "Teko",
    "h6-font-size": "1",
    "h1-font-size": 3,
    "h2-font-size": "2",
    "h3-font-size": "1.75",
    "h4-font-size": "1.5",
    "h5-font-size": "1.25"
  },
  "spacing": {
    "logo-height": 3,
    "section-padding": "2",
    "section-margins": "3",
    "sidebar-padding": "2",
    "content-cols": "6",
    "sidebar-cols": "4"
  }
}
